{
  "name": "Porta Viewer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Porta Viewer Extension",
  "homepage_url": "http://example.com",
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "css": [
        "src/inject/css/jquery-ui.css"
      ],
      "js": [
        "src/inject/js/jquery.js",
        "src/inject/js/jquery-ui.js",
        "src/inject/js/gaussian.js",
        "src/inject/js/sidebar-extension.js",
        "src/inject/js/bottombar-extension.js",
        "src/inject/js/main-extension.js",
        "src/inject/js/iframe.js"
      ]
    }
  ]
}