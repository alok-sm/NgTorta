{
  "name": "Porta Viewer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Porta Viewer Extension",
  "homepage_url": "http://example.com",
  "default_locale": "en",
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "src/inject/js/jquery.js",
        "src/inject/js/main.js"
      ],
      "css" : []
    },
    {
      "matches": [
        "http://localhost:3000/*"
      ],
      "js": [
        "src/inject/js/jquery.js",
        "src/inject/js/qtip.js",
        "src/inject/js/sidebarUtils.js",
        "src/inject/js/sidebar.js"
      ],
      "css" : [
        "src/inject/css/qtip.css"
      ]
    }
  ]
}