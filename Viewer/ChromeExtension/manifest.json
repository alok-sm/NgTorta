{
  "name": "Porta Viewer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Porta Viewer Extension",
  "homepage_url": "http://example.com",
  "default_locale": "en",
  "permissions": [
    "*://*/*"
  ],
   "background": {
    "persistent": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "js": [
        "src/inject/js/jquery.js",
        "src/inject/js/qtip.js",
        "src/inject/js/sidebarUtils.js",
        "src/inject/js/messaging.js",
        "src/inject/js/sidebar-extension.js",
        "src/inject/js/main-extension.js",
        "src/inject/js/iframe.js"
      ],
      "css" : [
        "src/inject/css/qtip.css"
      ]
    }
  ]
}